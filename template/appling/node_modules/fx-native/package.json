{
  "name": "fx-native",
  "version": "1.1.4",
  "description": "libfx bindings for JavaScript",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js"
  },
  "imports": {
    "events": {
      "bare": "bare-events",
      "default": "events"
    }
  },
  "files": [
    "index.js",
    "binding.c",
    "binding.js",
    "CMakeLists.txt",
    "lib",
    "prebuilds"
  ],
  "addon": true,
  "scripts": {
    "test": "npm run lint && npm run test:bare && npm run test:node",
    "test:bare": "bare test.js",
    "test:node": "node test.js",
    "lint": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/fx-native.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/fx-native/issues"
  },
  "homepage": "https://github.com/holepunchto/fx-native#readme",
  "dependencies": {
    "bare-events": "^2.5.4",
    "require-addon": "^1.1.0"
  },
  "devDependencies": {
    "brittle": "^3.1.0",
    "cmake-bare": "^1.6.1",
    "cmake-fetch": "^1.4.6",
    "prettier": "^3.5.3",
    "prettier-config-standard": "^7.0.0"
  }
}
