{
  "name": "drive-bundler",
  "version": "2.6.0",
  "description": "Extract bundles from drives",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "workspaces": [
    "test/fixtures/*"
  ],
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "fs/*": {
      "bare": "bare-fs/*",
      "default": "fs/*"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check . && lunte",
    "test": "npm run test:node",
    "test:node": "brittle-node --coverage test.js"
  },
  "dependencies": {
    "b4a": "^1.6.6",
    "dependency-stream": "^2.0.0",
    "mutexify": "^1.4.0",
    "sodium-native": "^5.0.0",
    "unix-path-resolve": "^1.0.2",
    "url-file-url": "^1.0.1"
  },
  "devDependencies": {
    "bare-bundle": "^1.0.0",
    "brittle": "^3.3.2",
    "localdrive": "^1.11.3",
    "lunte": "^1.0.0",
    "node-bare-bundle": "^1.4.2",
    "prettier": "^3.6.2",
    "prettier-config-holepunch": "^2.0.0"
  },
  "optionalDependencies": {
    "bare-fs": "^4.0.1",
    "bare-path": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holepunchto/drive-bundler.git"
  },
  "author": "Holepunch Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/drive-bundler/issues"
  },
  "homepage": "https://github.com/holepunchto/drive-bundler"
}
